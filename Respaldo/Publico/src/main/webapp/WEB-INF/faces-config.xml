<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xi="http://www.w3.org/2001/XInclude" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
	<application>
		<view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
		<locale-config>
			<default-locale>es_MX</default-locale>
			<supported-locale>en_US</supported-locale>
		</locale-config>
		<resource-bundle>
			<base-name>cde.publico.messages</base-name>
			<var>msg</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>cde.publico.Entorno</base-name>
			<var>entorno</var>
		</resource-bundle>
	</application>
	
	<managed-bean>
		<managed-bean-name>appMediator</managed-bean-name>
		<managed-bean-class>net.giro.navegador.AppMediator</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>navegador</property-name>
			<value>#{navegador}</value>
		</managed-property>
		<managed-property>
			<property-name>loginManager</property-name>
			<value>#{loginManager}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>localeManager</managed-bean-name>
		<managed-bean-class>net.giro.navegador.LocaleManager</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>loginManager</managed-bean-name>
		<managed-bean-class>net.giro.navegador.LoginManager</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>navegador</managed-bean-name>
		<managed-bean-class>net.giro.navegador.Navegador</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>estadosAction</managed-bean-name>
		<managed-bean-class>cde.publico.ubicacion.EstadosAction</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>municipiosAction</managed-bean-name>
		<managed-bean-class>cde.publico.ubicacion.MunicipiosAction</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>localidadesAction</managed-bean-name>
		<managed-bean-class>cde.publico.LocalidadesAction</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>coloniasAction</managed-bean-name>
		<managed-bean-class>cde.publico.ubicacion.ColoniasAction</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>regionesAction</managed-bean-name>
		<managed-bean-class>cde.publico.ubicacion.RegionesAction</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>PerfilesValoresAction</managed-bean-name>
		<managed-bean-class>cde.publico.PerfilesValoresAction</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>sucursalesAction</managed-bean-name>
		<managed-bean-class>cde.publico.ne.SucursalesAction</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>FuncionesAction</managed-bean-name>
		<managed-bean-class>cde.publico.FuncionesAction</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>perfilesAction</managed-bean-name>
		<managed-bean-class>cde.publico.PerfilesAction</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>MenusAction</managed-bean-name>
		<managed-bean-class>cde.publico.MenusAction</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ResponsabilidadesAction</managed-bean-name>
		<managed-bean-class>cde.publico.ResponsabilidadAction</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>conGrupoValoresAction</managed-bean-name>
		<managed-bean-class>cde.publico.GrupoValoresAction</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>valoresxGrupoAction</managed-bean-name>
		<managed-bean-class>cde.publico.ValoresxGrupoAction</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>UsuarioAction</managed-bean-name>
		<managed-bean-class>cde.publico.UsuarioAction</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>especialistasAction</managed-bean-name>
		<managed-bean-class>cde.publico.EspecialistasAction</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>empresasAction</managed-bean-name>
		<managed-bean-class>cde.publico.EmpresasAction</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ImpuestosAction</managed-bean-name>
		<managed-bean-class>cde.publico.ne.ImpuestosAction</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>subImpAction</managed-bean-name>
		<managed-bean-class>cde.publico.ne.SubfamiliaImpuestosAction</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<validator>
		<validator-id>email</validator-id>
		<validator-class>cde.publico.validator.EmailValidator</validator-class>
	</validator>
	<validator>
		<validator-id>contrasena</validator-id>
		<validator-class>cde.publico.validator.ContrasenaValidator</validator-class>
	</validator>
	<validator>
		<validator-id>valClaveValor</validator-id>
		<validator-class>cde.publico.validator.IdValorValidator</validator-class>
	</validator>
	<validator>
		<validator-id>strClaveValor</validator-id>
		<validator-class>cde.publico.validator.StrValorValidator</validator-class>
	</validator>
	<validator>
		<validator-id>SoloNum</validator-id>
		<validator-class>cde.publico.validator.SoloNumerosValidator</validator-class>
	</validator>
</faces-config>