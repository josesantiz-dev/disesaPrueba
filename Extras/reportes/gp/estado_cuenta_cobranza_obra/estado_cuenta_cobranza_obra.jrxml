	<queryString>
		<![CDATA[SELECT  o.id_obra
    , o.nombre AS obra_nombre
    , o.nombre_responsable
    , o.nombre_cliente
    , COALESCE((
            SELECT det.monto
            FROM presupuesto pre INNER JOIN presupuesto_detalle det ON det.id_presupuesto = pre.id
            WHERE pre.id_obra = o.id_obra AND det.id_concepto_presupuesto = 16)
        , 0) AS monto_contratado_con_iva
    , cob.fecha
    , cob.folio AS factura
    , cob.concepto
    , cob.anticipo
    , cob.estimacion AS monto_estimacion
    , cob.amortizacion AS amortizacion_anticipo
    , cob.fondo_garantia
    , cob.subtotal AS subtotal_factura
    , cob.iva AS iva_factura
    , cob.total AS total_factura
    , cob.total AS total_cobrado
    , COALESCE((cob.total - cob.total), 0) AS por_cobrar
    , cob.porcentaje_anticipo
    , cob.porcentaje_retencion
    , COALESCE(0) AS por_ejercer
    , CURRENT_DATE AS fecha_emision
    , pe.aa id_perfil_iva
    , (COALESCE(CAST(pv.ai AS double precision), 0) / 100) AS valor_perfil_iva
FROM obra o
    INNER JOIN obra_cobranza cob ON cob.id_obra = o.id_obra
    , d7729f32ba7 pe INNER JOIN b761110ccfe pv ON pv.af = pe.aa AND pe.af = 'VALOR_IVA'
WHERE cast(o.id_obra as varchar) = $P{idObra};]]>
	</queryString>
