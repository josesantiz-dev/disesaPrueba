	<queryString>
		<![CDATA[SELECT o.id_obra
    , o.nombre as nombre_proyecto
    , o.nombre_cliente
    , o.nombre_contrato
    , o.objeto_contrato
    , con.af AS estatus_contrato
    , o.domicilio
    , CASE lower(o.satic02) WHEN 'true'  THEN 'X' ELSE '' END AS satic02_si
    , CASE lower(o.satic02) WHEN 'false' THEN 'X' ELSE '' END AS satic02_no
    , o.monto_anticipo
    , COALESCE((o.monto_contratado / o.monto_anticipo), 0) as porcentaje_anticipo
    , o.monto_contratado
    , o.fecha_inicio
    , o.fecha_terminacion
    , pre.id AS id_presupuesto
    , pre.tema
    , pre.ruta
    , pre.monto_total
FROM obra o
    LEFT  JOIN presupuesto pre ON pre.id_obra = o.id_obra AND (SELECT COUNT(*) FROM presupuesto_detalle WHERE id_presupuesto = pre.id) = 16
    LEFT  JOIN de7a4d94446 con ON con.aa = o.estatus_contrato
WHERE cast(o.id_obra as varchar) = $P{idObra};]]>
	</queryString>
